modules = ["nodejs-20", "web", "bun", "bun-1.2", "postgresql-16"]
run = "npm run dev"

[nix]
channel = "stable-25_05"
packages = ["openssl", "imagemagick", "librsvg"]

[deployment]
run = ["bash", "-c", "cd .next/standalone && PORT=5000 HOSTNAME=0.0.0.0 node server.js"]
build = ["bash", "-c", "bunx prisma generate && bun run build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/"]
deploymentTarget = "autoscale"

